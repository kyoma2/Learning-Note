--直接插入排序--
  insertsort(int A[]){
      for(int i=2,i<=n,i++){   ////!!!!!!!! i从2开始，因为只需要n-1趟
          if (A[i]<A[i-1]){   ///如果已有序,continue.
              A[0] = A[i];
              for (int j = i-1,A[0]<A[j];j--) 
                  A[j+1] = A[j];
              A[j+1] = A[0];
          }
      }
  }
